# See http://editorconfig.org/ for docs on this file

root = true

[*]
indent_style = space

[*.{cs,shader,cginc}]
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{md,markdown}]
end_of_line = lf
charset = utf-8
# Trailing whitespace is significant in markdown
trim_trailing_whitespace = false
